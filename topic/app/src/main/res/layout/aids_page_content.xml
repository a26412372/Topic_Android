<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:background="#c0c0c0"
        android:textSize="25dp"
        android:id="@+id/AD_page_aids_unit"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="40dp" />

    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/AD_page_aids_nature"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/AD_page_aids_organ"
        android:gravity="center"
        android:layout_width="420dp"
        android:layout_height="wrap_content" />



    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/AD_page_aids_processing_unit"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:background="#f0f0f0"
        android:layout_marginTop="10dp"
        android:id="@+id/AD_page_aids_phone"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:background="#f0f0f0"
        android:layout_marginTop="0dp"
        android:id="@+id/AD_page_aids_fax"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/AD_page_aids_address"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/AD_page_aids_X"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_marginTop="10dp"
        android:id="@+id/AD_page_aids_Y"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />



</LinearLayout>